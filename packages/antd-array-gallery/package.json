{
  "name": "@canner/antd-array-gallery",
  "version": "0.0.49",
  "description": "array gallery",
  "main": "lib/index.js",
  "keywords": [
    "cms",
    "gallery",
    "canner"
  ],
  "scripts": {
    "test": "../../node_modules/.bin/jest"
  },
  "author": "canner",
  "license": "ISC",
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(@canner/image-upload)/)"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0 || ^16.0.0"
  },
  "dependencies": {
    "@canner/image-service-config": "^0.0.8",
    "@canner/image-upload": "^0.6.0",
    "grid-breakpoint": "^1.1.2",
    "grid-draggable": "^1.1.2",
    "immutable": "^3.8.2"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1"
  }
}
